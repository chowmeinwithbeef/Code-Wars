function alternate(n, firstValue, secondValue){
  let arr = []
  arr.push(firstValue)
  
  for (let i = 1; i < n; i++) {
      if (arr[i-1] == firstValue){
        arr.push(secondValue)
      } else
        arr.push(firstValue)
  }
  return n==0 ? [] : arr
}
// .push() adds to end of array
// ternary condition ? if true : if false
